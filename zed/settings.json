{
  "buffer_font_features": {
    "calt": 0
  },
  "buffer_font_family": "Iosevka",
  "ui_font_family": "Inter Display",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "lsp": {
    "protobuf-language-server": {
      "binary": {
        "path": "buf",
        "arguments": ["beta", "lsp"]
      }
    }
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": {
    "mode": "dark",
    "light": "Nord Light",
    "dark": "Nord"
  }
}
